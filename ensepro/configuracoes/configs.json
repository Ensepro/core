{
    "frases": {
        "arquivo": "arquivos/frases/frases.txt",
        "regex": {
            "palavra_relevante": ":(adj|n|prop)$",
            "palavra_adjetivo": ":adj$",
            "palavra_substantivo": ":n$",
            "palavra_preposicao": ":prop$",
            "palavra_verbo": "v-",
            "voz_passiva": "^fApass:pp$"
        },
        "verbos_de_ligacao": [
            "ser"
        ]
    },
    "sinonimos": {
        "linguagens": [
            "por",
            "eng"
        ]
    },
    "servidores": {
        "palavras": {
            "endpoint": "http://127.0.0.1",
            "porta": "8099",
            "servicos": {
                "analisar_frase": "/palavras/analisar/"
            }
        },
        "elastic_search": {
            "endpoint": "127.0.0.1",
            "porta": "9200",
            "settings": {
                "index_name": "triples",
                "index_type": "triple"
            }
        }
    },
    "logger": {
        "nome_arquivo": "ensepro_log.log",
        "modo_arquivo": "w",
        "formato": "%(asctime)s - %(name)s - %(levelname)s - %(message)s",
        "modulos": {
            "arvore": {
                "nivel": "INFO"
            },
            "configuracoes": {
                "nivel": "INFO"
            },
            "nlu": {
                "nivel": "INFO",
                "complementos_nominais": {
                    "nivel": "INFO"
                },
                "locucao_verbal": {
                    "nivel": "INFO"
                },
                "tipo_frases": {
                    "nivel": "INFO"
                },
                "voz": {
                    "nivel": "INFO"
                }
            },
            "servicos": {
                "nivel": "INFO",
                "palavras_service": {
                    "nivel": "INFO"
                }
            },
            "sinonimos": {
                "nivel": "INFO"
            }
        }
    }
}